aws cloudformation create-stack \
    --template-body file://bootstrap.template \
    --stack-name tools-stack-prod \
    --capabilities CAPABILITY_IAM \
    --parameters \
        ParameterKey=BastionKeyName,ParameterValue=tools-access \
        ParameterKey=SSHFrom,ParameterValue=10.0.10.0/32 \
        ParameterKey=VPCName,ParameterValue=Tools